<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title><%= page.title ? page.title + ' | ' : '' %><%= config.title %></title>

<!-- Google Fonts: Roboto & Material Symbols Outlined -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" rel="stylesheet" />

<!-- Theme CSS -->
<%- css('css/theme.css') %>
<%- css('css/main.css') %>

<!-- Material Web Components & Dynamic Color Utility -->
<script type="module" src="https://cdn.jsdelivr.net/npm/@material/web@1.0.0-pre.11/dist/all.js" defer></script>
<script type="module" src="https://cdn.jsdelivr.net/npm/@material/material-color-utilities@0.3.0/+esm" defer></script>

<!-- Main Theme JS -->
<%- js('js/main.js') %>